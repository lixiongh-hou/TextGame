<?xml version="1.0" encoding="utf-8"?>
<com.kwad.sdk.widget.KSFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <com.kwad.components.ad.interstitial.aggregate.TransViewPager
    android:id="@+id/ksad_multi_ad_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false" />

  <com.kwad.components.ad.interstitial.aggregate.InterstitialAggregateManualTipsView
    android:id="@+id/ksad_manual_tips_view"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="bottom|center_horizontal"
    android:layout_marginBottom="10dp"
    android:visibility="gone"
    tools:visibility="visible" />

  <com.kwad.components.ad.interstitial.aggregate.ViewPagerIndicator
    android:id="@+id/ksad_multi_ad_indicator"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:visibility="gone"
    tools:visibility="visible"
    android:layout_gravity="bottom|center_horizontal"
    android:layout_marginBottom="0dp" />

  <com.kwad.components.ad.interstitial.aggregate.SlideTipsView
    android:id="@+id/ksad_left_slide"
    android:visibility="gone"
    tools:visibility="visible"
    android:layout_gravity="right|center_vertical"
    app:ksad_is_left_slide="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>

  <com.kwad.components.ad.interstitial.aggregate.SlideTipsView
    android:id="@+id/ksad_right_slide"
    android:visibility="gone"
    tools:visibility="visible"
    android:layout_gravity="left|center_vertical"
    app:ksad_is_left_slide="false"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>

</com.kwad.sdk.widget.KSFrameLayout>